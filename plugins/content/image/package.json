{
  "name": "@serlo-org/ory-editor-plugins-image",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "author": "ORY GmbH",
  "license": "GNU Lesser General Public License v3.0",
  "scripts": {
    "build": "npm-run-all --parallel build:lib build:css",
    "build:watch": "npm-run-all --parallel build:lib:watch build:css:watch",
    "build:lib": "babel ./src --source-maps --ignore test.js --out-dir ./lib",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:css":
      "postcss -c ../../../../postcss.config.js --dir lib/ src/index.css",
    "build:css:watch": "npm run build:css -- -w",
    "docs":
      "documentation build ./src/**/*.js --format md --github -o ../../../../docs/api/plugins/image.md",
    "prebuild": "npm run clean",
    "clean": "rimraf \"lib/*\"",
    "flow:generate": "flow-typed install  --flowVersion=0.46.0"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "material-ui": "^0.19.1"
  }
}
